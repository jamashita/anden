{
  "name": "@jamashita/anden",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/anden.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run --parallel build",
    "clean": "rimraf coverage && lerna run --parallel clean",
    "commit": "git cz",
    "format": "eslint --fix 'packages/*/src/**/*.{ts,tsx}'",
    "release": "lerna exec --concurrency 1 -- semantic-release -e semantic-release-monorepo --tag-format='${LERNA_PACKAGE_NAME}-v\\${version}'",
    "setup": "lerna bootstrap && lerna link",
    "test": "jest test",
    "update": "yarn update:upgrade && yarn update:install && yarn setup",
    "update:install": "yarn install && yarn upgrade",
    "update:upgrade": "ncu --upgrade"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@jamashita/eslint-config": "1.25.3",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "26.0.20",
    "@types/object-hash": "1.3.4",
    "@types/sinon": "9.0.10",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "@typescript-eslint/typescript-estree": "4.15.0",
    "concurrently": "5.3.0",
    "eslint": "7.19.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "git-cz": "4.7.6",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "lint-staged": "10.5.4",
    "npm-check-updates": "11.1.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.3.8",
    "semantic-release-monorepo": "7.0.4",
    "sinon": "9.2.4",
    "sort-package-json": "1.48.1",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
