{
  "name": "@jamashita/anden-helper",
  "version": "1.6.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/anden.git/packages/helper"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && tsc -b tsconfig.build.json",
    "clean": "concurrently \"npm:clean:*\"",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean:others": "rimraf *.log"
  },
  "engines": {
    "node": ">= 15.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
