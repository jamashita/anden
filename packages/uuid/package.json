{
  "name": "@jamashita/anden-uuid",
  "version": "1.2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/anden.git/packages/uuid"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "concurrently \"npm:clean:*\"",
    "clean:converage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "update": "yarn update:upgrade && yarn update:install && yarn setup",
    "update:install": "yarn install && yarn upgrade",
    "update:upgrade": "ncu --upgrade"
  },
  "dependencies": {
    "@jamashita/anden-error": "^1.1.0",
    "@jamashita/anden-object": "^1.1.0",
    "@jamashita/anden-type": "^1.2.0",
    "reflect-metadata": "0.1.13",
    "uuid": "8.3.2"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
