{
  "name": "@jamashita/anden-zeit",
  "version": "1.12.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/anden.git/packages/zeit"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && tsc -b tsconfig.build.json",
    "clean": "concurrently \"npm:clean:*\"",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean:others": "rimraf *.log",
    "test": "jest test"
  },
  "dependencies": {
    "@jamashita/anden-error": "^1.10.0",
    "@jamashita/anden-object": "^1.11.0",
    "@jamashita/anden-type": "^1.10.1",
    "dayjs": "1.10.6",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "sinon": "11.1.1"
  },
  "engines": {
    "node": ">= 15.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
